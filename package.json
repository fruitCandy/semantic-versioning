{
    "devDependencies": {
        "@commitlint/cli": "^6.2.0",
        "@commitlint/config-conventional": "^6.1.3",
        "alpha-cz-1": "^0.1.0",
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "beta-changelog": "^0.1.1",
        "beta-cz": "^0.1.0",
        "commitizen": "^2.9.6",
        "cross-env": "^5.1.5",
        "fs-extra": "^6.0.1",
        "husky": "^0.14.3",
        "latest-version": "^3.1.0",
        "lerna": "^2.11.0",
        "load-json-file": "^5.0.0",
        "npm": "^6.0.1",
        "path": "^0.12.7",
        "promise": "^8.0.1"
    },
    "name": "semantic-versioning",
    "version": "1.0.0",
    "main": "index.js",
    "private": true,
    "scripts": {
        "test": "scripts/test",
        "semantic-release": "lerna publish --conventional-commits --yes",
        "bumpTest": "lerna publish --skip-npm --conventional-commits",
        "commit": "git cz",
        "bootstrap": "lerna bootstrap",
        "build:js": "cross-env BABEL_ENV=production babel --out-dir=dist ./utils/bump.js --copy-files",
        "bumpCorgix": "npm run build:js && node ./dist/utils/bump.js"
    },
    "workspaces": [
        "packages/*"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/alpha-cz-1"
        }
    }
}
