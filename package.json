{
    "devDependencies": {
        "@commitlint/cli": "^6.2.0",
        "@commitlint/config-conventional": "^6.1.3",
        "@commitlint/config-lerna-scopes": "^6.1.3",
        "@commitlint/prompt-cli": "^6.1.3",
        "@commitlint/travis-cli": "^6.2.0",
        "alpha-cz": "^0.1.0",
        "beta-changelog": "^0.1.0",
        "commitizen": "^2.9.6",
        "cz-customizable": "^5.2.0",
        "husky": "^0.14.3",
        "lerna": "^3.0.0-beta.15",
        "lerna-semantic-release": "^9.0.1"
    },
    "private": true,
    "name": "semantic-versioning",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "test": "scripts/test",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "semantic-release": "lerna publish --conventional-commits --yes",
        "lerna": "lerna publish --skip-npm --conventional-commits --changelog-preset beta-changelog",
        "commit": "git cz",
        "bootstrap": "lerna bootstrap"
    },
    "workspaces": [
        "packages/*"
    ],
    "config": {
        "commitizen": {
            "path": "./utils/beta-cz"
        }
    }
}
